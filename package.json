{
  "name": "@jsxiaosi/xs-cli",
  "version": "0.0.3",
  "description": "cli模版创建工具",
  "main": "./bin/index.mjs",
  "bin": "./bin/index.mjs",
  "bugs": {
    "url": "https://github.com/jsxiaosi/xstmpl/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsxiaosi/xstmpl.git"
  },
  "exports": {
    ".": "./dist/index.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "unbuild",
    "lint:pretty": "pretty-quick --staged",
