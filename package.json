  "author": "",
  "license": "MIT",
  "dependencies": {
    "giget": "^0.1.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^9.0.2",
    "@types/node": "^18.8.4",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "chalk": "^5.1.0",
    "commander": "^9.4.1",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "inquirer": "^9.1.3",
    "lint-staged": "^13.0.3",
    "ora": "^6.1.2",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rx": "^4.1.0",
    "rxjs": "6.6.7",
    "typescript": "^4.8.4",
    "unbuild": "^0.8.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "lint-staged": {
    "˜{src}/**/*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
